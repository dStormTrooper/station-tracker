{
	"name": "css-station-tracker-react",
	"version": "1.0.0",
	"description": "空间站位置追踪器 - React版本",
	"scripts": {
		"dev": "webpack --mode=development --watch",
		"build": "webpack --mode=production",
		"build:mobile": "BUILD_TARGET=mobile webpack --mode=production && npx cap sync",
		"build:tailwind": "tailwindcss -i ./src/styles/input.css -o ./src/styles/styles.css --watch",
		"type-check": "tsc --noEmit",
		"cap:add:android": "npx cap add android",
		"cap:add:ios": "npx cap add ios",
		"cap:sync": "npx cap sync",
		"cap:run:android": "npx cap run android",
		"cap:run:ios": "npx cap run ios",
		"cap:open:android": "npx cap open android",
		"cap:open:ios": "npx cap open ios"
	},
	"dependencies": {
		"@capacitor/android": "^5.5.1",
		"@capacitor/app": "^5.0.6",
		"@capacitor/core": "^5.5.1",
		"@capacitor/haptics": "^5.0.6",
		"@capacitor/ios": "^5.5.1",
		"@capacitor/keyboard": "^5.0.6",
		"@capacitor/status-bar": "^5.0.6",
		"leaflet": "^1.9.4",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"satellite.js": "^6.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.23.0",
		"@babel/preset-react": "^7.22.0",
		"@babel/preset-typescript": "^7.23.0",
		"@capacitor/cli": "^5.5.1",
		"@types/leaflet": "^1.9.8",
		"@types/react": "^18.2.0",
		"@types/react-dom": "^18.2.0",
		"babel-loader": "^9.1.0",
		"copy-webpack-plugin": "^11.0.0",
		"css-loader": "^6.8.0",
		"html-webpack-plugin": "^5.5.0",
		"style-loader": "^3.3.0",
		"tailwindcss": "^3.3.0",
		"ts-loader": "^9.5.0",
		"typescript": "^5.2.0",
		"webpack": "^5.88.0",
		"webpack-cli": "^5.1.0"
	},
	"packageManager": "pnpm@8.0.0"
}
