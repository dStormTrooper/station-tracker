{
	"manifest_version": 3,
	"name": "天宫空间站位置监控器",
	"version": "1.0",
	"description": "实时监控中国天宫空间站的轨道位置",
	"permissions": ["storage"],
	"host_permissions": [
		"https://celestrak.org/*",
		"https://*.tile.openstreetmap.org/*",
		"https://celestrak.com/*"
	],
	"action": {
		"default_popup": "popup.html",
		"default_title": "天宫空间站位置",
		"default_icon": {
			"16": "icons/icon16.png",
			"48": "icons/icon48.png",
			"128": "icons/icon128.png"
		}
	},
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	}
}
